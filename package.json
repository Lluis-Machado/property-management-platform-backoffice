    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@nivo/bar": "0.80.0",
    "@nivo/core": "0.80.0",
    "@nivo/tooltip": "0.80.0",
    "allotment": "1.19.0",
    "classnames": "2.3.2",
    "devextreme": "23.1.3",
    "exceljs": "4.3.0",
    "file-saver": "2.0.5",
    "formik": "2.2.9",
    "framer-motion": "10.12.16",
    "iron-session": "6.3.1",
    "jszip": "^3.10.1",
    "jwt-decode": "3.1.2",
    "luxon": "3.3.0",
    "negotiator": "0.6.3",
    "next": "13.4.18",
    "pg-components": "0.3.3",
    "react-content-loader": "6.2.1",
    "react-toastify": "9.1.3",
    "swr": "2.1.5",
    "yup": "1.2.0",
    "zustand": "4.3.8"
  },
  "resolutions": {
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5"
  },
  "devDependencies": {
    "@types/file-saver": "2.0.5",
    "@types/luxon": "3.3.0",
    "@types/negotiator": "0.6.1",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "autoprefixer": "10.4.14",
    "devextreme-react": "23.1.3",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.6",
