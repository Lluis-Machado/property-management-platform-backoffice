    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.24",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "0.0.1",
    "sharp": "0.32.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  }
